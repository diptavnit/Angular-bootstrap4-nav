{
  "name": "angular-bootstrap4-nav",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build":
      "ng build --progress --verbose  --common-chunk --vendor-chunk --extract-css --progress --named-chunks",
    "build-watch":
      "ng build --progress --verbose --vendor-chunk --extract-css --named-chunks --watch",
    "build-prod":
      "ng build --progress --verbose --common-chunk --vendor-chunk --extract-css --named-chunks --prod --progress",
    "start":
      "ng serve --open --vendor-chunk --extract-css --common-chunk --named-chunks  --progress ",
    "start-prod":
      "ng serve --open --vendor-chunk --extract-css --common-chunk --named-chunks --prod --aot --progress",
    "test": "ng test",
    "lint": "ng lint --typeCheck --format stylish",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.10",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@angular/router": "5.2.10",
    "bootstrap": "^4.1.0",
    "core-js": "^2.0.0",
    "jquery": "^3.0.0",
    "ngx-bootstrap": "^2.0.0",
    "popper.js": "^1.14.3",
    "rxjs": "^5.5.10",
    "tether": "^1.4.4",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.7.4",
    "@angular/compiler-cli": "5.2.10",
    "@angular/language-service": "5.2.10",
    "@types/jasmine": "^2.0.0",
    "@types/jasminewd2": "^2.0.0",
    "@types/node": "^8.0.0",
    "codelyzer": "^4.0.0",
    "jasmine-core": "^2.0.0",
    "jasmine-spec-reporter": "^4.0.0",
    "karma": "^1.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.0",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "^1.0.0",
    "karma-jasmine-html-reporter": "^0.2.0",
    "protractor": "^5.0.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.0.0",
    "typescript": "2.6.2"
  }
}
